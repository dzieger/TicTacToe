<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" 
		xmlns="http://javafx.com/javafx/8.0.171" 
		xmlns:fx="http://javafx.com/fxml/1" 
		fx:controller="com.dzieger.tictactoe.MainMenuController">
   <children>
      <Label text="Welcome to Tic-Tac-Toe" />
      <Button fx:id="playRegular" text="Play Regular Tic-Tac-Toe" onAction="#playRegularGame"/>
      <Button fx:id="playUltimate" text="Play Ultimate Tic-Tac-Toe" onAction="#playUltimateGame"/>
      <Button fx:id="quitGame" text="Quit" onAction="#quitGame"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
